{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/eslint-recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
        "no-console": 2,
        "indent": [
            "warn",
            4
        ],
        "no-unused-vars": 0,
        "@typescript-eslint/no-unused-vars": 0
    }
}